{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": "eslint:recommended",
  "rules": {
    "semi": 0, //;
    "no-extra-semi": 0,
    "no-unused-vars": 0,
    "no-redeclare": 0
  }
}